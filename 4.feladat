def piglatinnyelv(mondat):
    mondat=mondat.lower()
    ujmondat = ""
    ujszo=""
    valasz=""
    szavak = mondat.split(" ")
    for j in szavak:
        elso = j[0]
        ujszo +=j[1:] + elso + "ay"+" "
    ujmondat+=ujszo
    nagybetu=ujmondat[0].upper()
    valasz+=nagybetu+ujmondat[1:]
    return valasz


mondat = "The quick brown fox"
print(mondat)
print(piglatinnyelv(mondat))
