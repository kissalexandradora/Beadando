def piglatinnyelv(mondat):
    ujmondat = ""
    ujszo=""
    szavak = mondat.split(" ")
    print(szavak)

    for j in szavak:
        elso = j[0]
        ujszo +=j + elso + "ay"+" "
    print(ujszo)
    ujmondat+=ujszo
    return ujmondat

mondat = "The quick brown fox"
print(mondat)
print(piglatinnyelv(mondat))
